    <addon id="repository.dojo" name="Dojo Streams" version="0.1.3" provider-name="dojo">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Dojo Streams">
            <info compressed="false">https://raw.githubusercontent.com/dejay313/dojostreams/master/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/dejay313/dojostreams/master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/dejay313/dojostreams/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>A short summary of this addon</summary>
            <description>The description can be longer. Using [CR] you can create a newline. The use of other markup is not advised.</description>
