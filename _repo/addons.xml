<addons>
<addon id="plugin.video.dojostreams" name="Dojo Streams" version="0.0.3" provider-name="Dojo">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.urlresolver"  version="3.0.0"/>
	<import addon="script.module.addon.common"/>
	<import addon="plugin.video.youtube" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
	<summary lang="en">Dojo Streams </summary>
	<description lang="en">A plugin for playing online lists</description>
	<disclaimer lang="en">The original author neither hosts or stores any content, the original author has no affiliation with content hosts</disclaimer>
	<platform>all</platform>
  </extension>
</addon>

